            <kind>1</kind>
            <string>wspeed=4
if (vsp &lt; 10){vsp+=grav}

k_space=keyboard_check_direct(vk_space)
if cheat=1{
    cx=x+16
    if cx &gt; 480{
        cx-=480
    }
    if place_free(cx,y-43){
        k_space=1
    }
}
if (place_meeting(x,y+1,obj_Wall)){
    vsp = k_space * -jspeed
}

//Change Sprites
if (vsp == 0 and !sprite_index == spr_Frog_Running){
    sprite_index=spr_Frog_Running
}else if (sign(vsp) == -1 and !sprite_index == spr_Frog_Jumping){
    sprite_index=spr_Frog_Jumping
}else if (!sprite_index == spr_Frog_Falling){
    sprite_index=spr_Frog_Falling
}

//Collision horizontal
if (place_meeting(x+wspeed,y,obj_Wall)){
    while(!place_meeting(x+1,y,obj_Wall)){
        x+=1
    }
    if place_meeting(x,y,obj_Wall){
        KillFrog(2)
    }
    wspeed=0
}

//Collision vertical
if (place_meeting(x,y+vsp,obj_Wall)){
    while(!place_meeting(x,y+sign(vsp),obj_Wall)){
        y += sign(vsp)
    }
    if (sign(vsp) == -1){
        KillFrog(1)
    }
    vsp=0
}

x+=wspeed
y+=vsp
py=room_height/2
if (y &lt; py){
    yover=py-y
    y=py
    obj_Wall.yoff+=yover
    tyover+=yover
    if tyover &gt;= 48{
        global.points+=1
        tyover-=48;
        genLayer(-48+tyover)
    }
}

if (y+16 &lt;= 192 &amp;&amp; global.points == 0){
    global.points=1
}else if (y &lt;= 144 &amp;&amp; global.points == 1 and !dead){
    global.points=2
}

</string>
